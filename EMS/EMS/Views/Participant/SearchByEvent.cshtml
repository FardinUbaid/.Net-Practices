
<h2>Participants for Event</h2>

@if (TempData["Msg"] != null)
{
    <div class="alert alert-warning">@TempData["Msg"]</div>
}

<table class="table">
    <thead>
        <tr>
            <th>Participant ID</th>
            <th>Name</th>
            <th>Contact</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var participant in Model)
        {
            <tr>
                <td>@participant.PId</td>
                <td>@participant.Name</td>
                <td>@participant.Contact</td>
            </tr>
        }
    </tbody>
</table>
